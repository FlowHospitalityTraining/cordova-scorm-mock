{
  "name": "uk.co.flowhospitalitytraining.flowzone",
  "version": "0.0.1",
  "displayName": "Flow Training",
  "scripts": {
    "kill-java": "cmd '/C TASKKILL /f /im java.exe'",
    "ionic:build": "echo 'Flow!'",
    "run-release": "phonegap run android --prod --release --buildConfig=build.json",
    "build-release": "phonegap build android --prod --release --buildConfig=build.json",
    "run-release-ios": "phonegap run ios --prod --release --buildConfig=build.json",
    "build-release-ios": "phonegap build ios --prod --release --buildConfig=build.json"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-dialogs": {},
      "cordova-plugin-file": {},
      "cordova-plugin-file-opener2": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "cordova-plugin-file-transfer": {},
      "cordova-plugin-globalization": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-vibration": {},
      "cordova-plugin-whitelist": {},
      "phonegap-plugin-push": {
        "ANDROID_SUPPORT_V13_VERSION": "27.+",
        "FCM_VERSION": "17.0.+"
      },
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-advanced-http": {},
      "@globules-io/cordova-plugin-ios-xhr": {}
    },
    "platforms": [
      "android",
      "browser"
    ]
  },
  "dependencies": {
    "cordova-ios": "^6.2.0"
  },
  "devDependencies": {
    "@globules-io/cordova-plugin-ios-xhr": "git+https://github.com/globules-io/cordova-plugin-ios-xhr.git",
